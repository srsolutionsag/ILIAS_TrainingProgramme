	<StyleParameter Name="background-position" Value="right 1px"/>
	<StyleParameter Name="background-repeat" Value="no-repeat"/>
	<StyleParameter Name="background-image" Value="nav_arr_R_s.png"/>
	<StyleParameter Name="width" Value="16px"/>
	<StyleParameter Name="height" Value="16px"/>
</Style>
<Style Tag="div" Type="page_fn" Class="Footnote">
	<StyleParameter Name="font-style" Value="italic"/>
	<StyleParameter Name="margin-bottom" Value="5px"/>
	<StyleParameter Name="margin-top" Value="5px"/>
</Style>

