<StyleSheet>
<Title>Default Style</Title>
<Description></Description>

<!-- Link -->
<Style Tag="a" Type="link" Class="ExtLink">
	<StyleParameter Name="text-decoration" Value="underline"/>
	<StyleParameter Name="font-weight" Value="normal"/>
	<StyleParameter Name="color" Value="blue"/>
</Style>

<Style Tag="a" Type="link" Class="FootnoteLink">
	<StyleParameter Name="font-weight" Value="normal"/>
	<StyleParameter Name="color" Value="blue"/>
</Style>
<Style Tag="a" Type="link" Class="IntLink">
	<StyleParameter Name="text-decoration" Value="underline"/>
	<StyleParameter Name="font-weight" Value="normal"/>
	<StyleParameter Name="color" Value="blue"/>
</Style>

<!-- List Item -->
<Style Tag="li" Type="list_item" Class="StandardListItem">
</Style>

<!-- Numbered List -->
<Style Tag="ol" Type="list_o" Class="NumberedList">
</Style>

<!-- Bulleted List -->
<Style Tag="ul" Type="list_u" Class="BulletedList">
</Style>

<!-- File List Container -->
<Style Tag="div" Type="flist_cont" Class="FileListContainer">
	<StyleParameter Name="margin-left" Value="0px"/>
	<StyleParameter Name="margin-bottom" Value="10px"/>
	<StyleParameter Name="margin-top" Value="10px"/>
	<StyleParameter Name="margin-right" Value="0px"/>
	<StyleParameter Name="border-style" Value="solid"/>
	<StyleParameter Name="border-color" Value="#9EADBA"/>
	<StyleParameter Name="border-width" Value="1px"/>
</Style>

<!-- File List Heading -->
<Style Tag="div" Type="flist_head" Class="FileListHeading">
	<StyleParameter Name="text-align" Value="left"/>
	<StyleParameter Name="font-weight" Value="bold"/>
	<StyleParameter Name="padding" Value="3px"/>
	<StyleParameter Name="background-color" Value="#E0E0E0"/>
</Style>

<!-- File List -->
<Style Tag="ul" Type="flist" Class="FileList">
	<StyleParameter Name="list-style-type" Value="none"/>
	<StyleParameter Name="padding" Value="0px"/>
	<StyleParameter Name="margin" Value="0px"/>
</Style>

<!-- File List Item -->
<Style Tag="li" Type="flist_li" Class="FileListItem">
	<StyleParameter Name="padding" Value="3px"/>
</Style>

<!-- Media Container -->
<Style Tag="table" Type="media_cont" Class="MediaContainer">
	<StyleParameter Name="margin" Value="10px"/>
	<StyleParameter Name="padding" Value="0px"/>
	<StyleParameter Name="background-color" Value="#F5F5F5"/>
	<StyleParameter Name="border-width" Value="1px"/>
	<StyleParameter Name="border-color" Value="#B0B0B0"/>
	<StyleParameter Name="border-style" Value="solid"/>
</Style>

<!-- Media Caption -->
<Style Tag="div" Type="media_caption" Class="MediaCaption">
	<StyleParameter Name="padding" Value="5px"/>
	<StyleParameter Name="font-size" Value="90%"/>
</Style>

<!-- Page -->
<Style Tag="table" Type="page_frame" Class="PageFrame">
	<StyleParameter Name="padding-left" Value="15px"/>
	<StyleParameter Name="padding-right" Value="15px"/>
	<StyleParameter Name="width" Value="100%"/>
</Style>
<Style Tag="table" Type="page_cont" Class="PageContainer">
	<StyleParameter Name="background-color" Value="#FFFFFF"/>
	<StyleParameter Name="border-width" Value="1px"/>
