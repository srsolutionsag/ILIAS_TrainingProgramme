</Style>
<Style Tag="a" Type="page_lnavlink" Class="LeftNavigationLink">
</Style>
<Style Tag="img" Type="page_lnavimage" Class="LeftNavigationImage">
	<StyleParameter Name="background-position" Value="left 1px"/>
	<StyleParameter Name="background-repeat" Value="no-repeat"/>
	<StyleParameter Name="background-image" Value="nav_arr_L_s.png"/>
	<StyleParameter Name="width" Value="16px"/>
	<StyleParameter Name="height" Value="16px"/>
</Style>
<Style Tag="div" Type="page_rnav" Class="RightNavigation">
