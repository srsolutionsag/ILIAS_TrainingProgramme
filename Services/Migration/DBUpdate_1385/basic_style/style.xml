	<StyleParameter Name="padding" Value="20px"/>
</Style>
<Style Tag="div" Type="page_title" Class="PageTitle">
	<StyleParameter Name="border-color" Value="#000000"/>
	<StyleParameter Name="border-bottom-style" Value="solid"/>
